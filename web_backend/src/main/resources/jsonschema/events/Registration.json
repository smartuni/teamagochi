{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Registration",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "endpoint": {
      "type": "string"
    },
    "registrationId": {
      "type": "string"
    },
    "registrationDate": {
      "type": "integer",
      "format": "date-time"
    },
    "lastUpdate": {
      "type": "integer",
      "format": "date-time"
    },
    "address": {
      "type": "string"
    },
    "lwM2mVersion": {
      "type": "string"
    },
    "lifetime": {
      "type": "integer"
    },
    "bindingMode": {
      "type": "string"
    },
    "rootPath": {
      "type": "string"
    },
    "objectLinks": {
      "type": "array",
      "items": {
        "$ref": "../common/ObjectLink.json"
      }
    },
    "secure": {
      "type": "boolean"
    },
    "additionalRegistrationAttributes": {
      "$ref": "../common/AdditionalAttributes.json"
    },
    "queuemode": {
      "type": "boolean"
    },
    "availableInstances": {
      "$ref": "../common/AvailableInstances.json"
    },
    "sleeping": {
      "type": "boolean"
    }
  },
  "required": [
    "additionalRegistrationAttributes",
    "address",
    "availableInstances",
    "bindingMode",
    "endpoint",
    "lastUpdate",
    "lifetime",
    "lwM2mVersion",
    "objectLinks",
    "queuemode",
    "registrationDate",
    "registrationId",
    "rootPath",
    "secure"
  ]
}
